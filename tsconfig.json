{
	"compileOnSave": false,
	"compilerOptions": {
		
		"moduleResolution": "node",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"declaration": true,
		"isolatedModules": true,
		"downlevelIteration": true,
		"outDir": "dist"
	},
	"ts-node": {
		"require": ["tsconfig-paths/register"]
	}
}